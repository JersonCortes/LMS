<div class="card mt-4">
	<div class="card-body">
		<div class="container">

			
		<div class="row align-items-start">
        		<form action="api/drop/" method="POST" >
			
				<div class="col">
        				<select class="form-select" id="userSelector"  name="register">
						
						<option value="" hidden>Seleccione un usuario</option>

						<% for(var i=0; i< students.length; i++){ %>	
								<option value="<%= students[i].registerNumber%>" > <%= students[i].registerNumber%></option>
						<% } %>
					</select>
        		 	</div>
			

					<div class="col-12 ">
 	   						<button type="submit"  class="btn btn-primary" >Dar de baja</button>
        				</div>
			</form>
        	</div>
	</div>
</div>

